{
  "name": "starter",
  "version": "1.0.0",
  "description": "start backend and frontend concurrently",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "backend": "nodemon backend/server.js",
    "frontend": "npm run front --prefix client",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd client && npm start\\\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm build --prefix client"
  },
  "author": "Yannick Choupo",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^6.2.0"
  },
  "engine": {
    "node": "v14.16.0",
    "npm": "6.14.11"
  }
}
